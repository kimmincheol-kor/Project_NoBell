<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nobell.owner">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.field.ReserveListActivity"></activity>
        <activity android:name=".activity.field.ReserveActivity" />
        <activity android:name=".activity.field.VisitActivity" />
        <activity android:name=".activity.office.restaurant.TableActivity" />
        <activity
            android:name=".activity.office.restaurant.EditTableActivity"
            android:theme="@style/MyAppTheme" />
        <activity
            android:name=".activity.office.restaurant.PlusTableActivity"
            android:theme="@style/MyAppTheme" />
        <activity android:name=".activity.office.restaurant.MenuActivity" />
        <activity
            android:name=".activity.office.restaurant.EditMenuActivity"
            android:theme="@style/MyAppTheme" />
        <activity
            android:name=".activity.office.restaurant.PlusMenuActivity"
            android:theme="@style/MyAppTheme" />
        <activity android:name=".activity.office.RestaurantActivity" />
        <activity android:name=".activity.office.OwnerActivity" />
        <activity android:name=".activity.SignupActivity" />
        <activity android:name=".activity.FieldActivity" />
        <activity android:name=".activity.OfficeActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>